// process.container = 'docker://dgarrimar/sqtlseekerfx:version1.1'
// singularity.enabled = true

process {
  executor = 'sge'
  queueSize = 1000
  clusterOptions = '-q rg-el7,long-sl7,short-sl7 -l virtual_free=10G,h_rt=6:00:00'
  $make_RData {
    clusterOptions = '-q rg-el7 -l virtual_free=50G,h_rt=24:00:00'
  }
  $prepare {
    clusterOptions = '-q rg-el7 -l virtual_free=50G,h_rt=24:00:00'
  } 
}
